{
    "name": "retinens/laravel-dashboard-calendar-tile",
    "description": "A calendar tile for Laravel Dashboard",
    "keywords": [
        "laravel-dashboard-calendar-tile",
        "laravel-dashboard-calendar-plus-tile"
    ],
    "homepage": "https://github.com/retinens/laravel-dashboard-calendar-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Lucas Houssa",
            "email": "lucas@retinens.com",
            "homepage": "https://retinens.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-dashboard": "^2.0",
        "spatie/laravel-google-calendar": "^2.5|^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Retinens\\CalendarTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Retinens\\CalendarTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Retinens\\CalendarTile\\CalendarTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
